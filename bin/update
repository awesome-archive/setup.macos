#!/bin/sh
# -*- mode: sh -*-
# vim: set ft=sh :

set -eu
cd "$(dirname "$0")"

brew upgrade
brew cleanup

vagrant plugin update

./update_docker

git -C "${ZDOTDIR:-$HOME}/.zprezto" pull --recurse-submodules
git -C "${ZDOTDIR:-$HOME}/.zprezto" gc --aggressive

./update_asdf
mas upgrade
./update_system
